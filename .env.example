# Firebase Admin SDK
# IMPORTANT: Copy this file to .env and fill in your actual values
# NEVER commit .env to Git!

FIREBASE_ADMIN_KEY={"type":"service_account","project_id":"YOUR_PROJECT_ID",...}

# Server Configuration
PORT=3000
NODE_ENV=development

# Render.com URL (will be set automatically on Render)
RENDER_EXTERNAL_URL=http://localhost:3000

# Rust+ Configuration (Optional - for testing)
RUSTPLUS_SERVER_IP=
RUSTPLUS_PORT=
RUSTPLUS_PLAYER_ID=
RUSTPLUS_PLAYER_TOKEN=
